{
  "name": "marketapi",
  "version": "1.0.0",
  "description": "Api para un market-store",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon src/app.js",
    "docker_database": "docker-compose up -d",
    "start": "node src/app.js",
    "start-fork": "node src/app.js -m fork",
    "start-cluster": "node src/app.js -m cluster",
    "pm2-fork": "pm2 start src/app.js --watch",
    "pm2-fork2": "pm2 start src/app.js -- --p=8080",
    "pm2-cluster": "pm2 start src/app.js --watch -i 8",
    "pm2-cluster2": "pm2 start src/app.js --name='Servidor2' -i 3 -- --p=8081",
    "lint": "esLint --ignore-path .gitignore . --ext .js",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "load-test": "artillery quick --count 100 -n 600 http://localhost:8080/api/productos",
    "test": "cross-env NODE_ENV=dev jest --config ./jest-e2e.config.json --verbose --detectOpenHandles --forceExit  --no-cache --detectOpenHandles --runInBand",
    "e2e:coverage": "cross-env NODE_ENV=dev jest --config ./jest-e2e.config.json --verbose --coverage --detectOpenHandles --forceExit  --no-cache --detectOpenHandles --runInBand"
  },
  "author": "Samir Mahmud",
  "license": "MIT",
  "dependencies": {
    "@firebase/firestore": "^3.7.3",
    "@google-cloud/firestore": "^6.0.0",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.33.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.14.2",
    "firebase-admin": "^11.0.1",
    "graylog2": "^0.2.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "minimist": "^1.2.7",
    "moment": "^2.29.4",
    "mongoose": "^6.7.2",
    "mongoose-sequence": "^5.3.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.2",
    "sqlite3": "^5.1.2",
    "twilio": "^4.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@jest/types": "^29.5.0",
    "@types/jest": "^29.4.4",
    "@types/node": "^18.15.3",
    "artillery": "^1.5.6",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "npm": "^9.6.2",
    "supertest": "^6.3.3"
  },
  "lint-staged": {
    "*.(js)": [
      "npm run lint-fix"
    ]
  }
}
